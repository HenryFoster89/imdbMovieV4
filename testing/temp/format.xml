<?xml version="1.0"?>
<BCPFORMAT xmlns="http://schemas.microsoft.com/sqlserver/2004/bulkload/format"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <RECORD>
        <FIELD ID="1" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="20"/>  <!-- tconst -->
        <FIELD ID="2" xsi:type="CharTerm" TERMINATOR="," MAX_LENGTH="30"/>  <!-- averageRating -->
        <FIELD ID="3" xsi:type="CharTerm" TERMINATOR="\n" MAX_LENGTH="30"/> <!-- numVotes -->
    </RECORD>
    <ROW>
        <COLUMN SOURCE="1" NAME="tconst" xsi:type="SQLVARYCHAR" LENGTH="20"/>
        <COLUMN SOURCE="2" NAME="averageRating" xsi:type="SQLVARYCHAR" LENGTH="30"/>
        <COLUMN SOURCE="3" NAME="numVotes" xsi:type="SQLVARYCHAR" LENGTH="30"/>
    </ROW>
</BCPFORMAT>
